<?xml version="1.0" encoding="UTF-8" ?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    <changeSet id="create_hits_table" author="VxDxK">
        <createTable tableName="s338999_hits_liquebase">
            <column autoIncrement="true" name="id" type="BIGINT">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="x" type="double"/>
            <column name="y" type="double"/>
            <column name="r" type="double"/>
            <column name="hit" type="bool"/>
            <column name="time" type="timestamp"/>
            <column name="processingTime" type="double"/>
        </createTable>
    </changeSet>
</databaseChangeLog>